{
  "name": "@restfulhead/auto-plugin-filter-by-workspace-path",
  "version": "0.0.1",
  "description": "A plugin for Intuit Auto that filters out commits based on the NPM workspace path",
  "keywords": [
    "auto",
    "plugin",
    "filter",
    "path",
    "npm",
    "workspace"
  ],
  "author": "Patrick Ruhkopf",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/restfulhead/npm-auto-plugins.git"
  },
  "homepage": "https://github.com/restfulhead/npm-auto-plugins",
  "bugs": {
    "url": "https://github.com/restfulhead/npm-auto-plugins/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "clean": "rimraf dist && rimraf tsconfig.tsbuildinfo",
    "prebuild": "npm run clean",
    "build": "tsc -b",
    "build:fast": "tsc -b --incremental",
    "watch": "tsc -w",
    "test": "tsc -b test/tsconfig.json && rm -rf coverage && jest",
    "lint": "npm run lint:src && npm run lint:test",
    "lint:src": "eslint -c '.eslintrc.js' './src/**/*.ts'",
    "lint:test": "eslint -c 'test/.eslintrc.js' './test/**/*.ts'",
    "lint:fix": "npm run lint:fix:src && npm run lint:fix:test",
    "lint:fix:src": "eslint --fix -c '.eslintrc.js' './src/**/*.ts'",
    "lint:fix:test": "eslint --fix -c 'test/.eslintrc.js' './test/**/*.ts'"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/index.d.ts"
      ]
    }
  },
  "main": "./dist/index.js",
  "dependencies": {
    "@auto-it/core": "11.0.4",
    "fp-ts": "^2.5.3",
    "io-ts": "^2.1.2",
    "tslib": "2.1.0"
  },
  "devDependencies": {
    "@types/env-ci": "^3.1.4",
    "@types/jest": "29.5.11",
    "@types/node": "^18.x",
    "@types/signale": "^1.4.7",
    "@typescript-eslint/eslint-plugin": "6.16.0",
    "@typescript-eslint/parser": "6.16.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.2",
    "eslint-plugin-unused-imports": "3.0.0",
    "jest": "29.7.0",
    "prettier": "3.1.1",
    "rimraf": "^5.0.5",
    "ts-jest": "29.1.1",
    "ts-loader": "9.5.1",
    "typescript": "5.3.3"
  },
  "auto": {
    "plugins": [
      "./dist/index.js",
      "npm"
    ],
    "labels": [
      {
        "name": "release-major",
        "changelogTitle": "ğŸ’¥ Major Change",
        "releaseType": "major"
      },
      {
        "name": "release-minor",
        "changelogTitle": "ğŸš€ Enhancement",
        "releaseType": "minor"
      },
      {
        "name": "release-patch",
        "changelogTitle": "ğŸ› Patch",
        "releaseType": "patch"
      },
      {
        "name": "release-internal",
        "changelogTitle": "ğŸ”© Internal",
        "releaseType": "patch"
      },
      {
        "name": "documentation",
        "changelogTitle": "ğŸ“ Documentation",
        "releaseType": "patch"
      }
    ]
  }
}
